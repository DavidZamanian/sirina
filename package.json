{
  "name": "sirina",
  "version": "0.1.0",
  "homepage": "https://davidzamanian.github.io/sirina",
  "private": true,
  "scripts": {
    "generate:sitemap": "node scripts/generate-sitemap.js",
    "generate:favicons": "node scripts/generate-favicons.mjs",
    "prebuild": "npm run generate:sitemap && npm run generate:favicons && node scripts/cache-bust-icons.mjs",
    "dev": "npm run generate:favicons && vite",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "npm run typecheck && npm run build",
    "deploy": "npm run build && gh-pages -d dist -b gh-pages --dotfiles"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@types/node": "^22.8.7",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "gh-pages": "^6.3.0",
    "sharp": "^0.34.3",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  }
}
